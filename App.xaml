<Application x:Class="BirthdayReminder.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BirthdayReminder"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="PartyImage" UriSource="/BirthdayReminder;component/Resources/party.png"/>
            <BitmapImage x:Key="CalendarImage" UriSource="/BirthdayReminder;component/Resources/calendar.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
